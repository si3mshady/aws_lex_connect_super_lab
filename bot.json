{
    "Type": "AWS::Lex::Bot",
    "Properties": {
      "AutoBuildBotLocales": true,
      
      "BotLocales": [
        {
          "LocaleId": "en_US",
          "VoiceSettings": {
            "VoiceId": "Joanna"
            
          },
          "NluConfidenceThreshold": 0.4
        }
      ],
      "BotTags": [
        {
          "Key": "Environment",
          "Value": "Production"
        }
      ],
      "DataPrivacy": {
        "ChildDirected": false
      },
      "Description": "My Amazon Lex V2 bot",
      "IdleSessionTTLInSeconds": 300,
      "Name": "MyLexBot",
      "RoleArn": "arn:aws:iam::335055665325:role/LexBotRoleV2",
      "TestBotAliasSettings": {},
      "TestBotAliasTags": []
    }
  }
  